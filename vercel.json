{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      }
    ],
    "rewrites": [
      {
        "source": "/api/(.*)",
        "destination": "/api/server.js"
      }
    ],
    "functions": {
        "api/*.js": { "memory": 512, "maxDuration": 30 }
      }
    }
  